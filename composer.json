{
    "name": "wacosta/wac-chat-funnels",
    "description": "Sistema de chat funnels conversacionales para WordPress",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "WACosta",
            "email": "contacto@wacosta.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "WAC\\ChatFunnels\\": "includes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
